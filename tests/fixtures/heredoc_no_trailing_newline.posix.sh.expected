#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; exit "$s"; fi; }
main() {
  {
    __sh2_loc="tests/fixtures/heredoc_no_trailing_newline.sh2:3:9"
    'cat'; __sh2_status=$?
    __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  } <<'__SH2_EOF__'
x
__SH2_EOF__
}
__sh2_status=0
main "$@"
