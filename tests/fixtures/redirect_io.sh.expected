main() {
  f="$( 'mktemp' )"
  {
    printf '%s\n' 'out'
  } > "$f"
  {
    printf '%s\n' 'err' >&2
  } 2>> "$f"
  'cat' "$f"
  {
    printf '%s\n' 'e2s' >&2
  } 2>&1
  infile="$( 'mktemp' )"
  echo hello > $infile
  {
    'cat'
  } < "$infile"
  f2="$( 'mktemp' )"
  {
    printf '%s\n' 'swap_ok'
  } 2> "$f2" 1>&2
  'cat' "$f2"
  'rm' '-f' "$f"
  'rm' '-f' "$infile"
  'rm' '-f' "$f2"
}

main "$@"

