#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; exit "$s"; fi; }
main() {
  if [ 'a' = 'b' ]; then
    printf '%s\n' 'no'
  elif [ 'a' = 'a' ]; then
    printf '%s\n' 'yes'
  else
    printf '%s\n' 'fallback'
  fi
}
__sh2_status=0
main "$@"
