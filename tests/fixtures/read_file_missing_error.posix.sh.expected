#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; exit "$s"; fi; }
__sh2_read_file() { cat "$1"; }
main() {
  __sh2_loc="tests/fixtures/read_file_missing_error.sh2:2:1"
  ignored="$( __sh2_read_file 'non_existent_file.txt' )"
  __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
}
__sh2_status=0
main "$@"
