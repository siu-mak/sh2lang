#!/usr/bin/env bash
__sh2_err_handler() { local s=$?; if [[ "${BASH_COMMAND}" == *"(exit "* ]]; then return $s; fi; printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; return $s; }
set -o errtrace
trap '__sh2_err_handler' ERR
main() {
  local __sh2_loc=""
  __sh2_loc="tests/fixtures/index_arith_index.sh2:2:5"
  xs=('a' 'b' 'c')
  __sh2_loc="tests/fixtures/index_arith_index.sh2:3:5"
  i="0"
  printf '%s\n' "${xs[( i + 2 )]}"
}
__sh2_status=0
main "$@"
