#!/usr/bin/env bash
__sh2_err_handler() { local s=$?; if [[ "${BASH_COMMAND}" == *"(exit "* ]]; then return $s; fi; printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; return $s; }
set -o errtrace
trap '__sh2_err_handler' ERR
main() {
  local __sh2_loc=""
  __sh2_loc="tests/fixtures/subshell_group_scope.sh2:2:3"
  x='0'
  __sh2_status=$?; (exit $__sh2_status)
  (
    __sh2_loc="tests/fixtures/subshell_group_scope.sh2:5:5"
    x='1'
    __sh2_status=$?; (exit $__sh2_status)
  )
  printf '%s\n' "$x"
  {
    __sh2_loc="tests/fixtures/subshell_group_scope.sh2:10:5"
    x='2'
    __sh2_status=$?; (exit $__sh2_status)
  }
  printf '%s\n' "$x"
}
__sh2_status=0
main "$@"
