#!/usr/bin/env bash
__sh2_err_handler() { local s=$?; if [[ "${BASH_COMMAND}" == *"(exit "* ]]; then return $s; fi; printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; return $s; }
set -o errtrace
trap '__sh2_err_handler' ERR
main() {
  local __sh2_loc=""
  __sh2_loc="tests/fixtures/while_basic.sh2:2:5"
  x='go'
  __sh2_status=$?; (exit $__sh2_status)
  while [ "$x" = 'go' ]; do
    printf '%s\n' 'loop'
    __sh2_loc="tests/fixtures/while_basic.sh2:5:9"
    x='stop'
    __sh2_status=$?; (exit $__sh2_status)
  done
}
__sh2_status=0
main "$@"
