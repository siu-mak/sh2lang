#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; exit "$s"; fi; }
main() {
  if [ "5" -gt "3" ]; then
    printf '%s\n' 'yes'
  fi
  if [ "2" -le "2" ]; then
    printf '%s\n' 'also yes'
  fi
}
__sh2_status=0
main "$@"
