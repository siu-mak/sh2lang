main() {
  case $- in *e*) __sh2_e=1;; *) __sh2_e=0;; esac; set +e
  if {
    {
      'false'; __sh2_status=$?; (exit $__sh2_status)
    }
  }; then
    if [ "${__sh2_e:-0}" = 1 ]; then set -e; fi
  else
    if [ "${__sh2_e:-0}" = 1 ]; then set -e; fi
    printf '%s\n' 'failed' >&2
  fi
}
__sh2_status=0
main "$@"
