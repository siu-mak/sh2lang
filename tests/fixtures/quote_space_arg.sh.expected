#!/usr/bin/env bash
__sh2_err_handler() { local s=$?; if [[ "${BASH_COMMAND}" == *"(exit "* ]]; then return $s; fi; printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; return $s; }
set -o errtrace
trap '__sh2_err_handler' ERR
main() {
  local __sh2_loc=""
  __sh2_loc="tests/fixtures/quote_space_arg.sh2:2:3"
  'sh' '-c' 'printf '\''%s\n'\'' "'"$1"'"' '_' 'a b'; __sh2_status=$?; (exit $__sh2_status)
}
__sh2_status=0
main "$@"
