#!/usr/bin/env bash
__sh2_check() { local s="$1"; local loc="$2"; if (( s != 0 )); then if [[ -n "$loc" ]]; then printf 'Error in %s\n' "$loc" >&2; fi; exit "$s"; fi; }
main() {
  if true; then
    printf '%s\n' 't_ok'
  else
    printf '%s\n' 't_bad'
  fi
  if false; then
    printf '%s\n' 'f_bad'
  else
    printf '%s\n' 'f_ok'
  fi
}
__sh2_status=0
main "$@"
