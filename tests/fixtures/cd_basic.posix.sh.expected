#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; mode="$3"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; if [ "$mode" = "return" ]; then return "$s"; else exit "$s"; fi; fi; }
main() {
  cd 'tmp'
  __sh2_loc="tests/fixtures/cd_basic.sh2:3:3"
  'pwd'; __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}" "exit"
  cd "$( 'pwd' )"
  __sh2_loc="tests/fixtures/cd_basic.sh2:5:3"
  'echo' 'done'; __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}" "exit"
}
__sh2_status=0
main "$@"
