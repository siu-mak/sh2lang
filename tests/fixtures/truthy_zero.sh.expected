#!/usr/bin/env bash
__sh2_check() { local s="$1"; local loc="$2"; local mode="$3"; if (( s != 0 )); then if [[ -n "$loc" ]]; then printf 'Error in %s\n' "$loc" >&2; fi; if [[ "$mode" == "return" ]]; then return "$s"; else exit "$s"; fi; fi; }
main() {
  if [ -n '0' ]; then
    printf '%s\n' 'T_STR_0'
  else
    printf '%s\n' 'F_STR_0'
  fi
  if [ -n "0" ]; then
    printf '%s\n' 'T_NUM_0'
  else
    printf '%s\n' 'F_NUM_0'
  fi
}
__sh2_status=0
main "$@"
