#!/usr/bin/env bash
__sh2_err_handler() {
  local s=$?
  local loc="${__sh2_loc:-}"
  if [[ "${BASH_COMMAND}" == *"(exit "* ]]; then return $s; fi
  if [[ -z "$loc" ]]; then return $s; fi
  if [[ "$loc" == "${__sh2_last_err_loc:-}" && "$s" == "${__sh2_last_err_status:-}" ]]; then return $s; fi
  __sh2_last_err_loc="$loc"
  __sh2_last_err_status="$s"
  printf "Error in %s\n" "$loc" >&2
  return $s
}
set -o errtrace
trap '__sh2_err_handler' ERR
main() {
  local __sh2_loc=""
  __sh2_loc="tests/fixtures/map_duplicate_keys.sh2:2:1"
  local -A m=( ['a']='1' ['a']='2' ['b']='3' )
  local -a __sh2_keys_m=( 'a' 'b' )
  __sh2_status=$?; (exit $__sh2_status)
  for __sh2_k in "${__sh2_keys_m[@]}"; do
    local k="$__sh2_k"
    local v="${m[$__sh2_k]}"
    printf '%s\n' "$k"
    printf '%s\n' "$v"
  done
  printf '%s\n' "${m['a']}"
}
__sh2_status=0
main "$@"
