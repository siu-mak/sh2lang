__sh2_err_handler() { printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; }
trap '__sh2_err_handler' ERR
main() {
  __sh2_loc="tests/fixtures/interpolated_dollar_escape.sh2:2:3"
  name='world'
  printf '%s\n' 'literal: {name}'
  printf '%s\n' 'dollar: $'
  printf '%s\n' 'interp: '"$name"
}
__sh2_status=0
main "$@"
