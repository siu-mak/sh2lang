main() {
  d="$( 'mktemp' '-d' )"
  (
    cd "$d"
    touch f.txt
  )
  f="$d"'/f.txt'
  if [ -e "$f" ] && [ -f "$f" ] && ! [ -d "$f" ]; then
    printf '%s\n' 'fs_ok'
  else
    printf '%s\n' 'fs_bad'
  fi
  cd "$d"
  if [ "$PWD" = "$d" ]; then
    printf '%s\n' 'cd_ok'
  else
    printf '%s\n' 'cd_bad'
  fi
  'rm' '-rf' "$d"
}

main "$@"

