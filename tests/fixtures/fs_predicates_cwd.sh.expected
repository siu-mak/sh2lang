    cd "$d"
    touch f.txt
  )
  __sh2_loc="tests/fixtures/fs_predicates_cwd.sh2:9:3"
  f="$d"'/f.txt'
  if [ -e "$f" ] && [ -f "$f" ] && ! [ -d "$f" ]; then
    printf '%s\n' 'fs_ok'
  else
    printf '%s\n' 'fs_bad'
  fi
  cd "$d"
