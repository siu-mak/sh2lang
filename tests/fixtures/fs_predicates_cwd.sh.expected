#!/usr/bin/env bash
__sh2_err_handler() {
  local s=$?
  local loc="${__sh2_loc:-}"
  if [[ "${BASH_COMMAND}" == *"(exit "* ]]; then return $s; fi
  if [[ -z "$loc" ]]; then return $s; fi
  if [[ "$loc" == "${__sh2_last_err_loc:-}" && "$s" == "${__sh2_last_err_status:-}" ]]; then return $s; fi
  __sh2_last_err_loc="$loc"
  __sh2_last_err_status="$s"
  printf "Error in %s\n" "$loc" >&2
  return $s
}
set -o errtrace
trap '__sh2_err_handler' ERR
main() {
  local __sh2_loc=""
  __sh2_loc="tests/fixtures/fs_predicates_cwd.sh2:3:3"
  d="$( 'mktemp' '-d' )"
  __sh2_status=$?; (exit $__sh2_status)
  (
    cd "$d"
    touch f.txt
    __sh2_status=$?; (exit $__sh2_status)
  )
  __sh2_loc="tests/fixtures/fs_predicates_cwd.sh2:9:3"
  f="$d"'/f.txt'
  __sh2_status=$?; (exit $__sh2_status)
  if [ -e "$f" ] && [ -f "$f" ] && ! [ -d "$f" ]; then
    printf '%s\n' 'fs_ok'
  else
    printf '%s\n' 'fs_bad'
  fi
  cd "$d"
  if [ "$PWD" = "$d" ]; then
    printf '%s\n' 'cd_ok'
  else
    printf '%s\n' 'cd_bad'
  fi
  __sh2_loc="tests/fixtures/fs_predicates_cwd.sh2:25:3"
  'rm' '-rf' "$d"; __sh2_status=$?; (exit $__sh2_status)
}
__sh2_status=0
main "$@"
