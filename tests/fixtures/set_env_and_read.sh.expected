#!/usr/bin/env bash
__sh2_err_handler() { local s=$?; if [[ "${BASH_COMMAND}" == *"(exit "* ]]; then return $s; fi; printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; return $s; }
set -o errtrace
trap '__sh2_err_handler' ERR
main() {
  local __sh2_loc=""
  export FOO='bar'
  printf '%s\n' "$( ( typeset +x FOO; printenv FOO ) 2>/dev/null || printenv FOO 2>/dev/null || true )"
  __sh2_loc="tests/fixtures/set_env_and_read.sh2:4:3"
  'sh' '-c' 'echo '"$FOO"; __sh2_status=$?; (exit $__sh2_status)
}
__sh2_status=0
main "$@"
