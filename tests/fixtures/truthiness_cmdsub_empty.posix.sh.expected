#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; exit "$s"; fi; }
main() {
  __sh2_loc="tests/fixtures/truthiness_cmdsub_empty.sh2:2:3"
  out="$( 'sh' '-c' 'printf '\''%s'\'' '\'''\''' )"
  __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  if [ -n "$out" ]; then
    printf '%s\n' 'bad'
  else
    printf '%s\n' 'ok'
  fi
}
__sh2_status=0
main "$@"
