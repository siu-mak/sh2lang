__sh2_err_handler() { printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; }
trap '__sh2_err_handler' ERR
main() {
  __sh2_loc="tests/fixtures/case_glob_dollar_and_backslash.sh2:2"
  x='cost $5\done'
  case "$x" in
    'cost $'*'\done')
      printf '%s\n' 'hit_safe'
    ;;
    *)
      printf '%s\n' 'miss'
    ;;
  esac
}
__sh2_status=0
main "$@"
