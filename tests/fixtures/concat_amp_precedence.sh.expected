#!/usr/bin/env bash
__sh2_err_handler() { local s=$?; if [[ "${BASH_COMMAND}" == *"(exit "* ]]; then return $s; fi; printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; return $s; }
set -o errtrace
trap '__sh2_err_handler' ERR
main() {
  local __sh2_loc=""
  __sh2_loc="tests/fixtures/concat_amp_precedence.sh2:2:3"
  s='n='"$(( ( 1 + 2 ) ))"
  __sh2_status=$?; (exit $__sh2_status)
  printf '%s\n' "$s"
}
__sh2_status=0
main "$@"
