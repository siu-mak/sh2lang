#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; exit "$s"; fi; }
main() {
  __sh2_loc="tests/fixtures/try_catch_nounset.sh2:3:5"
  'set' '-u'; __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  case $- in *e*) __sh2_e=1;; *) __sh2_e=0;; esac; set +e
  if {
    {
      'sh' '-c' 'exit 77'; __sh2_status=$?
      (exit $__sh2_status)
    }
  }; then
    if [ "${__sh2_e:-0}" = 1 ]; then set -e; fi
  else
    if [ "${__sh2_e:-0}" = 1 ]; then set -e; fi
    printf '%s\n' 'caught'
  fi
  printf '%s\n' 'done'
}
__sh2_status=0
main "$@"
