test() {
  printf '%s\n' "$#"
  printf '%s\n' "$1"
  printf '%s\n' "$3"
  printf '%s\n' "$( arr=("$@"); idx=$(( 0 )); printf "%s" "${arr[idx]}" )"
  printf '%s\n' "$( arr=("$@"); idx=$(( 2 )); printf "%s" "${arr[idx]}" )"
  printf '%s\n' "$( IFS='-'; printf "%s" "$*" )"
  printf '%s\n' "$#"
  for a in "$@"; do
    printf '%s\n' "$a"
  done
}

main() {
  xs=('a' 'b' 'c')
  printf '%s\n' "${#xs[@]}"
  printf '%s\n' "${xs[1]}"
  printf '%s\n' "$( IFS='-'; printf "%s" "${xs[*]}" )"
  for item in "${xs[@]}"; do
    printf '%s\n' "$item"
  done
  test 'x' 'y' 'z'
}

main "$@"

