__sh2_err_handler() { printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; }
trap '__sh2_err_handler' ERR
test() {
  printf '%s\n' "$#"
  printf '%s\n' "$1"
  printf '%s\n' "$3"
  printf '%s\n' "$( arr=("$@"); idx=$(( 0 )); printf "%s" "${arr[idx]}" )"
  printf '%s\n' "$( arr=("$@"); idx=$(( 2 )); printf "%s" "${arr[idx]}" )"
  printf '%s\n' "$( IFS='-'; printf "%s" "$*" )"
  printf '%s\n' "$#"
  for a in "$@"; do
    printf '%s\n' "$a"
  done
}

main() {
  __sh2_loc="/srv/repos/rust/sh2c/tests/fixtures/list_args_utils.sh2:23"
  xs=('a' 'b' 'c')
  printf '%s\n' "${#xs[@]}"
  printf '%s\n' "${xs[1]}"
  printf '%s\n' "$( IFS='-'; printf "%s" "${xs[*]}" )"
  for item in "${xs[@]}"; do
    printf '%s\n' "$item"
  done
  test 'x' 'y' 'z'; __sh2_status=$?; (exit $__sh2_status)
}
__sh2_status=0
main "$@"
