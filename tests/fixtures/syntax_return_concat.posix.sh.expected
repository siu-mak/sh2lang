#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; exit "$s"; fi; }
BASE() {
  printf '%s' '/base'
  return 0
}

ENVS_DIR() {
  printf '%s' "$( BASE  )"'/envs'
  return 0
}

main() {
  __sh2_loc="tests/fixtures/syntax_return_concat.sh2:10:5"
  'echo' "$( ENVS_DIR  )"; __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
}
__sh2_status=0
main "$@"
