#!/usr/bin/env bash
__sh2_check() { local s="$1"; local loc="$2"; if (( s != 0 )); then if [[ -n "$loc" ]]; then printf 'Error in %s\n' "$loc" >&2; fi; exit "$s"; fi; }
is_yes() {
  if [ "1" = "1" ]; then printf '%s' 1; fi
  return 0
}

is_no() {
  if [ "1" = "0" ]; then printf '%s' 1; fi
  return 0
}

main() {
  if [ -n "$( is_yes  )" ]; then
    printf '%s\n' 'Y'
  fi
  if [ -n "$( is_no  )" ]; then
    printf '%s\n' 'N'
  fi
}
__sh2_status=0
main "$@"
