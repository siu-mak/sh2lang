#!/usr/bin/env bash
__sh2_require() { for c in "$@"; do if ! command -v -- "$c" >/dev/null 2>&1; then printf '%s\n' "missing required command: $c" >&2; exit 127; fi; done; }
main() {
  __sh2_require 'missingcmd'
  __sh2_status=$?; (exit $__sh2_status)
  printf '%s\n' 'after'
}
__sh2_status=0
main "$@"
