#!/usr/bin/env bash
__sh2_err_handler() { local s=$?; if [[ "${BASH_COMMAND}" == *"(exit "* ]]; then return $s; fi; printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; return $s; }
set -o errtrace
trap '__sh2_err_handler' ERR
main() {
  local __sh2_loc=""
  __sh2_loc="tests/fixtures/compare.sh2:2:5"
  user='admin'
  __sh2_status=$?; (exit $__sh2_status)
  if [ "$user" = 'admin' ]; then
    printf '%s\n' 'welcome root'
  fi
  if [ "$user" != 'guest' ]; then
    printf '%s\n' 'not guest'
  fi
}
__sh2_status=0
main "$@"
