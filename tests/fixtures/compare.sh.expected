__sh2_err_handler() { local s=$?; printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; return $s; }
trap '__sh2_err_handler' ERR
main() {
  __sh2_loc="tests/fixtures/compare.sh2:2:5"
  user='admin'
  if [ "$user" = 'admin' ]; then
    printf '%s\n' 'welcome root'
  fi
  if [ "$user" != 'guest' ]; then
    printf '%s\n' 'not guest'
  fi
}
__sh2_status=0
main "$@"
