#!/usr/bin/env bash
__sh2_err_handler() { local s=$?; if [[ "${BASH_COMMAND}" == *"(exit "* ]]; then return $s; fi; printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; return $s; }
set -o errtrace
trap '__sh2_err_handler' ERR
main() {
  local __sh2_loc=""
  __sh2_loc="tests/fixtures/self_pid_arith.sh2:3:3"
  x="$(( ( $$ + 1 ) ))"
  if [ "$x" -gt "$$" ]; then
    printf '%s\n' 'arith_ok'
  else
    printf '%s\n' 'arith_bad'
  fi
}
__sh2_status=0
main "$@"
