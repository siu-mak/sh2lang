#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; exit "$s"; fi; }
main() {
  __sh2_loc="tests/fixtures/compare.sh2:2:5"
  user='admin'
  __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  if [ "$user" = 'admin' ]; then
    printf '%s\n' 'welcome root'
  fi
  if [ "$user" != 'guest' ]; then
    printf '%s\n' 'not guest'
  fi
}
__sh2_status=0
main "$@"
