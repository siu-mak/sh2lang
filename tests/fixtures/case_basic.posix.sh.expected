#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; exit "$s"; fi; }
main() {
  __sh2_loc="tests/fixtures/case_basic.sh2:2:5"
  x='b'
  __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  case "$x" in
    'a')
      printf '%s\n' 'A'
    ;;
    'b'|'c')
      printf '%s\n' 'B or C'
    ;;
    *)
      printf '%s\n' 'default'
    ;;
  esac
}
__sh2_status=0
main "$@"
