#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; exit "$s"; fi; }
main() {
  __sh2_loc="tests/fixtures/interpolated_dollar_basic.sh2:2:3"
  name='world'
  __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  printf '%s\n' 'hello '"$name"'!'
}
__sh2_status=0
main "$@"
