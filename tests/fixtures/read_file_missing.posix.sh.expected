#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; exit "$s"; fi; }
__sh2_read_file() { cat "$1"; }
main() {
  __sh2_loc="tests/fixtures/read_file_missing.sh2:3:5"
  s="$( __sh2_read_file 'does_not_exist_xyz.txt' )"
  __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  if [ "$s" = '' ]; then
    printf '%s\n' 'empty'
  else
    printf '%s\n' 'not empty'
  fi
}
__sh2_status=0
main "$@"
