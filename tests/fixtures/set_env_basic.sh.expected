__sh2_err_handler() { printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; }
trap '__sh2_err_handler' ERR
main() {
  export FOO='bar'
  printf '%s\n' "${FOO}"
  __sh2_loc="tests/fixtures/set_env_basic.sh2:4"
  'sh' '-c' 'test "'"$FOO"'" = bar && echo child_ok'; __sh2_status=$?; (exit $__sh2_status)
}
__sh2_status=0
main "$@"
