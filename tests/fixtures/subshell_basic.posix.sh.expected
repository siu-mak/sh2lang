#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; exit "$s"; fi; }
main() {
  __sh2_loc="tests/fixtures/subshell_basic.sh2:2:5"
  x='outer'
  __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  (
    __sh2_loc="tests/fixtures/subshell_basic.sh2:4:9"
    x='inner'
    __sh2_status=$?
    __sh2_check "$__sh2_status" "${__sh2_loc:-}"
    printf '%s\n' "$x"
  )
  printf '%s\n' "$x"
}
__sh2_status=0
main "$@"
