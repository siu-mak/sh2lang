#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; exit "$s"; fi; }
main() {
  {
    __sh2_loc="tests/fixtures/with_redirect_stdin_and_stdout_file.sh2:3:5"
    'cat'; __sh2_status=$?
    __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  } < 'in.txt' > 'out.txt'
}
__sh2_status=0
main "$@"
