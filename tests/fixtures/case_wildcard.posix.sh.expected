#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; exit "$s"; fi; }
main() {
  __sh2_loc="tests/fixtures/case_wildcard.sh2:2:3"
  x='foo'
  __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  case "$x" in
    'bar')
      printf '%s\n' 'no'
    ;;
    *)
      printf '%s\n' 'yes'
    ;;
  esac
}
__sh2_status=0
main "$@"
