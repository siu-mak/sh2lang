#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; exit "$s"; fi; }
main() {
  if [ "1" -lt "2" ]; then
    printf '%s\n' 'lt_ok'
  else
    printf '%s\n' 'lt_bad'
  fi
  if [ "2" -le "2" ]; then
    printf '%s\n' 'le_ok'
  else
    printf '%s\n' 'le_bad'
  fi
  if [ "3" -gt "2" ]; then
    printf '%s\n' 'gt_ok'
  else
    printf '%s\n' 'gt_bad'
  fi
  if [ "3" -ge "3" ]; then
    printf '%s\n' 'ge_ok'
  else
    printf '%s\n' 'ge_bad'
  fi
}
__sh2_status=0
main "$@"
