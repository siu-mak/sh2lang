__sh2_err_handler() { local s=$?; if [[ "${BASH_COMMAND}" == *"(exit "* ]]; then return $s; fi; printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; return $s; }
set -o errtrace
trap '__sh2_err_handler' ERR
main() {
  local __sh2_loc=""
  __sh2_loc="tests/fixtures/len_basic.sh2:2:3"
  s='hello'
  printf '%s\n' "$( printf "%s" "$s" | awk 'BEGIN{l=0} {l=length($0)} END{print l}' )"
}
__sh2_status=0
main "$@"
