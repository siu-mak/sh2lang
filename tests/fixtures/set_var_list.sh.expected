#!/usr/bin/env bash
__sh2_err_handler() { local s=$?; if [[ "${BASH_COMMAND}" == *"(exit "* ]]; then return $s; fi; printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; return $s; }
set -o errtrace
trap '__sh2_err_handler' ERR
main() {
  local __sh2_loc=""
  __sh2_loc="tests/fixtures/set_var_list.sh2:2:3"
  xs=('a')
  __sh2_loc="tests/fixtures/set_var_list.sh2:3:3"
  xs=('a' 'b' 'c')
  printf '%s\n' "${xs[1]}"
}
__sh2_status=0
main "$@"
