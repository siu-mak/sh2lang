#!/usr/bin/env bash
__sh2_err_handler() {
  local s=$?
  local loc="${__sh2_loc:-}"
  if [[ "${BASH_COMMAND}" == *"(exit "* ]]; then return $s; fi
  if [[ -z "$loc" ]]; then return $s; fi
  if [[ "$loc" == "${__sh2_last_err_loc:-}" && "$s" == "${__sh2_last_err_status:-}" ]]; then return $s; fi
  __sh2_last_err_loc="$loc"
  __sh2_last_err_status="$s"
  printf "Error in %s\n" "$loc" >&2
  return $s
}
set -o errtrace
trap '__sh2_err_handler' ERR
main() {
  local __sh2_loc=""
  export X='hello'
  __sh2_loc="tests/fixtures/env_export_unset_source.sh2:4:3"
  'sh' '-c' 'echo $X'; __sh2_status=$?; (exit $__sh2_status)
  __sh2_loc="tests/fixtures/env_export_unset_source.sh2:7:3"
  y='yo'
  __sh2_status=$?; (exit $__sh2_status)
  export y
  __sh2_loc="tests/fixtures/env_export_unset_source.sh2:9:3"
  'sh' '-c' 'echo $y'; __sh2_status=$?; (exit $__sh2_status)
  unset X
  __sh2_loc="tests/fixtures/env_export_unset_source.sh2:13:3"
  'sh' '-c' 'test -z "$X" && echo unset_ok || echo unset_bad'; __sh2_status=$?; (exit $__sh2_status)
  __sh2_loc="tests/fixtures/env_export_unset_source.sh2:16:3"
  f="$( 'mktemp' )"
  __sh2_status=$?; (exit $__sh2_status)
  echo 'echo sourced_ok' > $f
  __sh2_status=$?; (exit $__sh2_status)
  . "$f"
  __sh2_loc="tests/fixtures/env_export_unset_source.sh2:21:3"
  'rm' '-f' "$f"; __sh2_status=$?; (exit $__sh2_status)
}
__sh2_status=0
main "$@"
