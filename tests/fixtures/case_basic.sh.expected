__sh2_err_handler() { printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; }
trap '__sh2_err_handler' ERR
main() {
  __sh2_loc="tests/fixtures/case_basic.sh2:2"
  x='b'
  case "$x" in
    'a')
      printf '%s\n' 'A'
    ;;
    'b'|'c')
      printf '%s\n' 'B or C'
    ;;
    *)
      printf '%s\n' 'default'
    ;;
  esac
}
__sh2_status=0
main "$@"
