#!/usr/bin/env bash
__sh2_err_handler() { local s=$?; if [[ "${BASH_COMMAND}" == *"(exit "* ]]; then return $s; fi; printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; return $s; }
set -o errtrace
trap '__sh2_err_handler' ERR
main() {
  local __sh2_loc=""
  __sh2_loc="tests/fixtures/case_basic.sh2:2:5"
  x='b'
  __sh2_status=$?; (exit $__sh2_status)
  case "$x" in
    'a')
      printf '%s\n' 'A'
    ;;
    'b'|'c')
      printf '%s\n' 'B or C'
    ;;
    *)
      printf '%s\n' 'default'
    ;;
  esac
}
__sh2_status=0
main "$@"
