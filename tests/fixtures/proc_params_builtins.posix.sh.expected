#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; mode="$3"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; if [ "$mode" = "return" ]; then return "$s"; else exit "$s"; fi; fi; }
main() {
  if [ "$0" = "$0" ]; then
    printf '%s\n' 'argv0_ok'
  else
    printf '%s\n' 'argv0_bad'
  fi
  if [ "$#" = "$#" ]; then
    printf '%s\n' 'argc_ok'
  else
    printf '%s\n' 'argc_bad'
  fi
  if [ "$$" = "${BASHPID}" ]; then
    printf '%s\n' 'self_pid_ok'
  else
    printf '%s\n' 'self_pid_bad'
  fi
}
__sh2_status=0
main "$@"
