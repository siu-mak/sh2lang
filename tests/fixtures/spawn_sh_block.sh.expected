#!/usr/bin/env bash
__sh2_check() { local s="$1"; local loc="$2"; if (( s != 0 )); then if [[ -n "$loc" ]]; then printf 'Error in %s\n' "$loc" >&2; fi; exit "$s"; fi; }
main() {
  ( 
    {
      echo one
      __sh2_status=$?
      __sh2_check "$__sh2_status" "${__sh2_loc:-}"
      echo two
      __sh2_status=$?
      __sh2_check "$__sh2_status" "${__sh2_loc:-}"
    }
  ) &
  wait; __sh2_status=$?
}
__sh2_status=0
main "$@"
