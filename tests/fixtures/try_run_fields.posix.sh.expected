#!/bin/sh
__sh2_check() { __sh2_s="$1"; __sh2_l="$2"; __sh2_m="$3"; if [ "$__sh2_s" -ne 0 ]; then if [ "$__sh2_m" = "return" ]; then return "$__sh2_s"; fi; if [ -n "$__sh2_l" ]; then printf 'Error in %s\n' "$__sh2_l" >&2; fi; exit "$__sh2_s"; fi; }
__sh2_tmpfile() { if command -v mktemp >/dev/null 2>&1; then mktemp; else printf "%s/sh2_tmp_%s_%s" "${TMPDIR:-/tmp}" "$$" "$(awk 'BEGIN{srand();print int(rand()*1000000)}')"; fi; }
__sh2_read_file() { cat "$1"; }
main() {
  __sh2_loc="tests/fixtures/try_run_fields.sh2:2:3"
    r__tmp_out="$(__sh2_tmpfile)"
  r__tmp_err="$(__sh2_tmpfile)"
  if 'sh' '-c' 'printf '\''out'\''; printf '\''err'\'' 1>&2; exit 7' >"${r__tmp_out}" 2>"${r__tmp_err}"; then r__status=0; else r__status=$?; fi;
  r__stdout="$(__sh2_read_file "$r__tmp_out")"
  r__stderr="$(__sh2_read_file "$r__tmp_err")"
  rm -f "$r__tmp_out" "$r__tmp_err"
  __sh2_status="$r__status"
  printf '%s\n' "$r__status"
  printf '%s\n' "$r__stdout"
  printf '%s\n' "$r__stderr"
}
__sh2_status=0
main "$@"
