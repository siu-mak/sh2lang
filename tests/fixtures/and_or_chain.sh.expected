#!/usr/bin/env bash
__sh2_err_handler() { local s=$?; if [[ "${BASH_COMMAND}" == *"(exit "* ]]; then return $s; fi; printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; return $s; }
set -o errtrace
trap '__sh2_err_handler' ERR
main() {
  local __sh2_loc=""
  {
    __sh2_loc="tests/fixtures/and_or_chain.sh2:2:3"
    'true'; __sh2_status=$?; (exit $__sh2_status)
  } && {
    printf '%s\n' 'ok'
  }
  {
    __sh2_loc="tests/fixtures/and_or_chain.sh2:3:3"
    'false'; __sh2_status=$?; (exit $__sh2_status)
  } || {
    printf '%s\n' 'fallback'
  }
}
__sh2_status=0
main "$@"
