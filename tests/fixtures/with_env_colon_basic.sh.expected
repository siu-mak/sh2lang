#!/usr/bin/env bash
__sh2_check() { local s="$1"; local loc="$2"; local mode="$3"; if (( s != 0 )); then if [[ -n "$loc" ]]; then printf 'Error in %s\n' "$loc" >&2; fi; if [[ "$mode" == "return" ]]; then return "$s"; else exit "$s"; fi; fi; }
main() {
  (
    export FOO='bar'
    export BAZ='qux'
    printf '%s\n' "$( ( unset FOO; printenv FOO ) 2>/dev/null || printenv FOO 2>/dev/null || true )"':'"$( ( unset BAZ; printenv BAZ ) 2>/dev/null || printenv BAZ 2>/dev/null || true )"
  )
}
__sh2_status=0
main "$@"
