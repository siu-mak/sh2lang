#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; exit "$s"; fi; }
main() {
  __sh2_loc="tests/fixtures/self_pid_arith.sh2:3:3"
  x="$(( ( $$ + 1 ) ))"
  __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  if [ "$x" -gt "$$" ]; then
    printf '%s\n' 'arith_ok'
  else
    printf '%s\n' 'arith_bad'
  fi
}
__sh2_status=0
main "$@"
