#!/usr/bin/env bash
__sh2_err_handler() { local s=$?; if [[ "${BASH_COMMAND}" == *"(exit "* ]]; then return $s; fi; printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; return $s; }
set -o errtrace
trap '__sh2_err_handler' ERR
main() {
  local __sh2_loc=""
  local msg="${1}"
  __sh2_loc="tests/fixtures/target_posix_basic.sh2:2:3"
  'echo' "$msg"; __sh2_status=$?; (exit $__sh2_status)
  VAR='set_via_env' 'print_env'
}

print_env() {
  local __sh2_loc=""
  __sh2_loc="tests/fixtures/target_posix_basic.sh2:9:3"
  'echo' "${VAR}"; __sh2_status=$?; (exit $__sh2_status)
}
__sh2_status=0
main "$@"
