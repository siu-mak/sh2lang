#!/usr/bin/env bash
__sh2_err_handler() { local s=$?; if [[ "${BASH_COMMAND}" == *"(exit "* ]]; then return $s; fi; printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; return $s; }
set -o errtrace
trap '__sh2_err_handler' ERR
main() {
  local __sh2_loc=""
  printf '%s\n' 'before'
  __sh2_loc="tests/fixtures/exec_basic.sh2:3:3"
  exec 'sh' '-c' 'echo execed'
  printf '%s\n' 'after'
}
__sh2_status=0
main "$@"
