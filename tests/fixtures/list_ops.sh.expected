__sh2_err_handler() { printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; }
trap '__sh2_err_handler' ERR
main() {
  __sh2_loc="tests/fixtures/list_ops.sh2:2"
  xs=('a' 'b' 'c')
  if [ "${#xs[@]}" = "3" ]; then
    printf '%s\n' 'count_ok'
  else
    printf '%s\n' 'count_bad'
  fi
  if [ "${xs[1]}" = 'b' ]; then
    printf '%s\n' 'index_ok'
  else
    printf '%s\n' 'index_bad'
  fi
  printf '%s\n' "$( IFS='-'; printf "%s" "${xs[*]}" )"
}
__sh2_status=0
main "$@"
