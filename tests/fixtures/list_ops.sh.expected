main() {
  xs=('a' 'b' 'c')
  if [ "${#xs[@]}" = "3" ]; then
    printf '%s\n' 'count_ok'
  else
    printf '%s\n' 'count_bad'
  fi
  if [ "${xs[1]}" = 'b' ]; then
    printf '%s\n' 'index_ok'
  else
    printf '%s\n' 'index_bad'
  fi
  printf '%s\n' "$( IFS='-'; printf "%s" "${xs[*]}" )"
}

main "$@"

