__sh2_err_handler() { printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; }
trap '__sh2_err_handler' ERR
__sh2_which() { command -v -- "$1" 2>/dev/null || true; }
main() {
  __sh2_loc="tests/fixtures/which_found.sh2:3"
  p="$( __sh2_which 'sh' )"
  if [ "$p" != '' ]; then
    printf '%s\n' 'OK'
  else
    printf '%s\n' 'NO'
  fi
}
__sh2_status=0
main "$@"
