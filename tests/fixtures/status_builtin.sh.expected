__sh2_err_handler() { printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; }
trap '__sh2_err_handler' ERR
main() {
  __sh2_loc="tests/fixtures/status_builtin.sh2:2"
  'true'; __sh2_status=$?; (exit $__sh2_status)
  if [ "$__sh2_status" = "0" ]; then
    printf '%s\n' 'true_ok'
  else
    printf '%s\n' 'true_bad'
  fi
  __sh2_loc="tests/fixtures/status_builtin.sh2:9"
  'false'; __sh2_status=$?; (exit $__sh2_status)
  if [ "$__sh2_status" != "0" ]; then
    printf '%s\n' 'false_ok'
  else
    printf '%s\n' 'false_bad'
  fi
}
__sh2_status=0
main "$@"
