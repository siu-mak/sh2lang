#!/usr/bin/env bash
__sh2_err_handler() { local s=$?; if [[ "${BASH_COMMAND}" == *"(exit "* ]]; then return $s; fi; printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; return $s; }
set -o errtrace
trap '__sh2_err_handler' ERR
main() {
  local __sh2_loc=""
  __sh2_loc="tests/fixtures/status_builtin.sh2:2:3"
  'true'; __sh2_status=$?; (exit $__sh2_status)
  if [ "$__sh2_status" = "0" ]; then
    printf '%s\n' 'true_ok'
  else
    printf '%s\n' 'true_bad'
  fi
  __sh2_loc="tests/fixtures/status_builtin.sh2:9:3"
  'false'; __sh2_status=$?; (exit $__sh2_status)
  if [ "$__sh2_status" != "0" ]; then
    printf '%s\n' 'false_ok'
  else
    printf '%s\n' 'false_bad'
  fi
}
__sh2_status=0
main "$@"
