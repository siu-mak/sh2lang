main() {
  'true'; __sh2_status=$?; (exit $__sh2_status)
  if [ "$__sh2_status" = "0" ]; then
    printf '%s\n' 'true_ok'
  else
    printf '%s\n' 'true_bad'
  fi
  'false'; __sh2_status=$?; (exit $__sh2_status)
  if [ "$__sh2_status" != "0" ]; then
    printf '%s\n' 'false_ok'
  else
    printf '%s\n' 'false_bad'
  fi
}

__sh2_status=0
main "$@"
