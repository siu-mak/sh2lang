f() {
  local a="${1}"
  local b="${2}"
  if [ "$#" = "2" ]; then
    printf '%s\n' 'argc_ok'
  else
    printf '%s\n' 'argc_bad'
  fi
  if [ "$( printf "%s" "$0" | awk 'BEGIN{l=0} {l=length($0)} END{print l}' )" -gt "0" ]; then
    printf '%s\n' 'argv0_ok'
  else
    printf '%s\n' 'argv0_bad'
  fi
}

main() {
  f 'x' 'y'; __sh2_status=$?; (exit $__sh2_status)
}
__sh2_status=0
main "$@"
