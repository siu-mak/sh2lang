main() {
  'mkdir' '-p' 'sandbox'; __sh2_status=$?; (exit $__sh2_status)
  (
    cd 'sandbox'
    'touch' 'f_empty'; __sh2_status=$?; (exit $__sh2_status)
    'touch' 'f_full'; __sh2_status=$?; (exit $__sh2_status)
    'sh' '-c' 'echo x > f_full'; __sh2_status=$?; (exit $__sh2_status)
    'ln' '-s' 'f_empty' 'link'; __sh2_status=$?; (exit $__sh2_status)
    'touch' 'f_exec'; __sh2_status=$?; (exit $__sh2_status)
    'chmod' '+x' 'f_exec'; __sh2_status=$?; (exit $__sh2_status)
    'touch' 'f_ro'; __sh2_status=$?; (exit $__sh2_status)
    'chmod' '444' 'f_ro'; __sh2_status=$?; (exit $__sh2_status)
    'touch' 'f_noread'; __sh2_status=$?; (exit $__sh2_status)
    'chmod' '000' 'f_noread'; __sh2_status=$?; (exit $__sh2_status)
    if [ -L 'link' ]; then
      printf '%s\n' 'link is symlink'
    fi
    if ! [ -L 'f_empty' ]; then
      printf '%s\n' 'f_empty is not symlink'
    fi
    if [ -s 'f_full' ]; then
      printf '%s\n' 'f_full is non_empty'
    fi
    if ! [ -s 'f_empty' ]; then
      printf '%s\n' 'f_empty is empty'
    fi
    if [ -x 'f_exec' ]; then
      printf '%s\n' 'f_exec is exec'
    fi
    if ! [ -x 'f_empty' ]; then
      printf '%s\n' 'f_empty is not exec'
    fi
    if [ -w 'f_empty' ]; then
      printf '%s\n' 'f_empty is writable'
    fi
    if ! [ -w 'f_ro' ]; then
      printf '%s\n' 'f_ro is not writable'
    fi
    if [ -r 'f_full' ]; then
      printf '%s\n' 'f_full is readable'
    fi
    if ! [ -r 'f_noread' ]; then
      printf '%s\n' 'f_noread is not readable'
    fi
  )
}

__sh2_status=0
main "$@"
