#!/usr/bin/env bash
__sh2_err_handler() {
  local s=$?
  local loc="${__sh2_loc:-}"
  if [[ "${BASH_COMMAND}" == *"(exit "* ]]; then return $s; fi
  if [[ -z "$loc" ]]; then return $s; fi
  if [[ "$loc" == "${__sh2_last_err_loc:-}" && "$s" == "${__sh2_last_err_status:-}" ]]; then return $s; fi
  __sh2_last_err_loc="$loc"
  __sh2_last_err_status="$s"
  printf "Error in %s\n" "$loc" >&2
  return $s
}
set -o errtrace
trap '__sh2_err_handler' ERR
main() {
  local __sh2_loc=""
  __sh2_loc="tests/fixtures/fs_predicates_full.sh2:2:5"
  'mkdir' '-p' 'sandbox'; __sh2_status=$?; (exit $__sh2_status)
  (
    cd 'sandbox'
    __sh2_loc="tests/fixtures/fs_predicates_full.sh2:4:9"
    'touch' 'f_empty'; __sh2_status=$?; (exit $__sh2_status)
    __sh2_loc="tests/fixtures/fs_predicates_full.sh2:5:9"
    'touch' 'f_full'; __sh2_status=$?; (exit $__sh2_status)
    __sh2_loc="tests/fixtures/fs_predicates_full.sh2:6:9"
    'sh' '-c' 'echo x > f_full'; __sh2_status=$?; (exit $__sh2_status)
    __sh2_loc="tests/fixtures/fs_predicates_full.sh2:7:9"
    'ln' '-s' 'f_empty' 'link'; __sh2_status=$?; (exit $__sh2_status)
    __sh2_loc="tests/fixtures/fs_predicates_full.sh2:9:9"
    'touch' 'f_exec'; __sh2_status=$?; (exit $__sh2_status)
    __sh2_loc="tests/fixtures/fs_predicates_full.sh2:10:9"
    'chmod' '+x' 'f_exec'; __sh2_status=$?; (exit $__sh2_status)
    __sh2_loc="tests/fixtures/fs_predicates_full.sh2:12:9"
    'touch' 'f_ro'; __sh2_status=$?; (exit $__sh2_status)
    __sh2_loc="tests/fixtures/fs_predicates_full.sh2:13:9"
    'chmod' '444' 'f_ro'; __sh2_status=$?; (exit $__sh2_status)
    __sh2_loc="tests/fixtures/fs_predicates_full.sh2:15:9"
    'touch' 'f_noread'; __sh2_status=$?; (exit $__sh2_status)
    __sh2_loc="tests/fixtures/fs_predicates_full.sh2:16:9"
    'chmod' '000' 'f_noread'; __sh2_status=$?; (exit $__sh2_status)
    if [ -L 'link' ]; then
      printf '%s\n' 'link is symlink'
    fi
    if ! [ -L 'f_empty' ]; then
      printf '%s\n' 'f_empty is not symlink'
    fi
    if [ -s 'f_full' ]; then
      printf '%s\n' 'f_full is non_empty'
    fi
    if ! [ -s 'f_empty' ]; then
      printf '%s\n' 'f_empty is empty'
    fi
    if [ -x 'f_exec' ]; then
      printf '%s\n' 'f_exec is exec'
    fi
    if ! [ -x 'f_empty' ]; then
      printf '%s\n' 'f_empty is not exec'
    fi
    if [ -w 'f_empty' ]; then
      printf '%s\n' 'f_empty is writable'
    fi
    if ! [ -w 'f_ro' ]; then
      printf '%s\n' 'f_ro is not writable'
    fi
    if [ -r 'f_full' ]; then
      printf '%s\n' 'f_full is readable'
    fi
    if ! [ -r 'f_noread' ]; then
      printf '%s\n' 'f_noread is not readable'
    fi
  )
}
__sh2_status=0
main "$@"
