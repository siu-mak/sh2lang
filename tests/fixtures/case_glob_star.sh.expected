#!/usr/bin/env bash
__sh2_err_handler() { local s=$?; if [[ "${BASH_COMMAND}" == *"(exit "* ]]; then return $s; fi; printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; return $s; }
set -o errtrace
trap '__sh2_err_handler' ERR
main() {
  local __sh2_loc=""
  __sh2_loc="tests/fixtures/case_glob_star.sh2:2:5"
  x='foo123'
  case "$x" in
    'foo'*)
      printf '%s\n' 'hit'
    ;;
    *)
      printf '%s\n' 'miss'
    ;;
  esac
}
__sh2_status=0
main "$@"
