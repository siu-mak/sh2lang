__sh2_err_handler() { local s=$?; printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; return $s; }
trap '__sh2_err_handler' ERR
main() {
  export FOO='bar'
  export BAZ
  unset FOO
  {
    __sh2_loc="tests/fixtures/export_unset.sh2:5:3"
    ( set -o pipefail; set +e; 'env' | 'grep' 'BAZ' ); __sh2_status=$?; (exit $__sh2_status)
  } || {
    printf '%s\n' 'no baz'
  }
}
__sh2_status=0
main "$@"
