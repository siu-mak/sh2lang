__sh2_err_handler() { printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; }
trap '__sh2_err_handler' ERR
main() {
  export FOO='bar'
  export BAZ
  unset FOO
  {
    __sh2_loc="tests/fixtures/export_unset.sh2:5:3"
    if [[ -o pipefail ]]; then __p=1; else __p=0; fi; set -o pipefail; case $- in *e*) __e=1;; *) __e=0;; esac; set +e; 'env' | 'grep' 'BAZ' ; __sh2_status=$?; if [ "$__e" = 1 ]; then set -e; fi; if [ "$__p" = 0 ]; then set +o pipefail; fi; (exit $__sh2_status)
  } || {
    printf '%s\n' 'no baz'
  }
}
__sh2_status=0
main "$@"
