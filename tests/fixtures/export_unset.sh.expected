__sh2_err_handler() { local s=$?; if [[ "${BASH_COMMAND}" == *"(exit "* ]]; then return $s; fi; printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; return $s; }
set -o errtrace
trap '__sh2_err_handler' ERR
main() {
  local __sh2_loc=""
  export FOO='bar'
  export BAZ
  unset FOO
  {
    __sh2_loc="tests/fixtures/export_unset.sh2:5:3"
    if ( set -o pipefail; set +e; 'env' | 'grep' 'BAZ' ); then __sh2_status=0; else __sh2_status=$?; fi; (exit $__sh2_status)
  } || {
    printf '%s\n' 'no baz'
  }
}
__sh2_status=0
main "$@"
