__sh2_err_handler() { printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; }
trap '__sh2_err_handler' ERR
main() {
  __sh2_loc="tests/fixtures/case_wildcard.sh2:2"
  x='foo'
  case "$x" in
    'bar')
      printf '%s\n' 'no'
    ;;
    *)
      printf '%s\n' 'yes'
    ;;
  esac
}
__sh2_status=0
main "$@"
