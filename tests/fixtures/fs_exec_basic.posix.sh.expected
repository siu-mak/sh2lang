#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; exit "$s"; fi; }
main() {
  __sh2_loc="tests/fixtures/fs_exec_basic.sh2:2:3"
  'sh' '-c' 'rm -f exe noexe; echo '\''echo hi'\'' > exe; chmod +x exe; echo '\''echo hi'\'' > noexe; chmod 644 noexe'; __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  if [ -x 'exe' ] && ! [ -x 'noexe' ]; then
    printf '%s\n' 'ok'
  else
    printf '%s\n' 'bad'
  fi
}
__sh2_status=0
main "$@"
