__sh2_err_handler() { printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; }
trap '__sh2_err_handler' ERR
main() {
  (
    __sh2_log_path='out.log'
    : > "$__sh2_log_path"
    trap 'exec >&-; exec 2>&-; wait' EXIT
    exec > >(tee -a "$__sh2_log_path")
    exec 2> >(tee -a "$__sh2_log_path" >&2)
    printf '%s\n' 'A'
    printf '%s\n' 'ERR' >&2
  )
  __sh2_loc="tests/fixtures/with_log_basic.sh2:7"
  'sh' '-c' 'sort out.log'; __sh2_status=$?; (exit $__sh2_status)
}
__sh2_status=0
main "$@"
