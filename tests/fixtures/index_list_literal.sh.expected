#!/usr/bin/env bash
__sh2_err_handler() { local s=$?; if [[ "${BASH_COMMAND}" == *"(exit "* ]]; then return $s; fi; printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; return $s; }
set -o errtrace
trap '__sh2_err_handler' ERR
main() {
  local __sh2_loc=""
  __sh2_loc="tests/fixtures/index_list_literal.sh2:2:3"
  x="$( arr=('a' 'b' 'c'); idx=$(( 1 )); printf "%s" "${arr[idx]}" )"
  printf '%s\n' "$x"
}
__sh2_status=0
main "$@"
