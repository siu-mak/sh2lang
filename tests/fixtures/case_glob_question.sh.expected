__sh2_err_handler() { printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; }
trap '__sh2_err_handler' ERR
main() {
  __sh2_loc="tests/fixtures/case_glob_question.sh2:2"
  x='ab'
  case "$x" in
    'a'?)
      printf '%s\n' 'hit_q'
    ;;
    'a'??)
      printf '%s\n' 'miss_qq'
    ;;
    *)
      printf '%s\n' 'miss'
    ;;
  esac
}
__sh2_status=0
main "$@"
