#!/usr/bin/env bash
main() {
  if [ "$0" = "$0" ]; then
    printf '%s\n' 'argv0_ok'
  else
    printf '%s\n' 'argv0_bad'
  fi
  if [ "$#" = "$#" ]; then
    printf '%s\n' 'argc_ok'
  else
    printf '%s\n' 'argc_bad'
  fi
  if [ "$$" = "${BASHPID}" ]; then
    printf '%s\n' 'self_pid_ok'
  else
    printf '%s\n' 'self_pid_bad'
  fi
}
__sh2_status=0
main "$@"
