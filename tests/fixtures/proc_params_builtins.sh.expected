#!/usr/bin/env bash
__sh2_check() { local s="$1"; local loc="$2"; local mode="$3"; if (( s != 0 )); then if [[ "$mode" == "return" ]]; then return "$s"; else if [[ -n "$loc" ]]; then printf 'Error in %s\n' "$loc" >&2; fi; exit "$s"; fi; fi; }
main() {
  if [ "$0" = "$0" ]; then
    printf '%s\n' 'argv0_ok'
  else
    printf '%s\n' 'argv0_bad'
  fi
  if [ "$#" = "$#" ]; then
    printf '%s\n' 'argc_ok'
  else
    printf '%s\n' 'argc_bad'
  fi
  if [ "$$" = "${BASHPID}" ]; then
    printf '%s\n' 'self_pid_ok'
  else
    printf '%s\n' 'self_pid_bad'
  fi
}
__sh2_status=0
main "$@"
