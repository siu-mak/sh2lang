__sh2_err_handler() { local s=$?; if [[ "${BASH_COMMAND}" == *"(exit "* ]]; then return $s; fi; printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; return $s; }
set -o errtrace
trap '__sh2_err_handler' ERR
main() {
  local __sh2_loc=""
  __sh2_loc="tests/fixtures/fs_non_empty_basic.sh2:2:5"
  'sh' '-c' 'rm -f empty nonempty; : > empty; echo hi > nonempty'; __sh2_status=$?; (exit $__sh2_status)
  if [ -s 'nonempty' ]; then
    printf '%s\n' 'nonempty_ok'
  else
    printf '%s\n' 'nonempty_bad'
  fi
  if ! [ -s 'empty' ]; then
    printf '%s\n' 'empty_ok'
  else
    printf '%s\n' 'empty_bad'
  fi
  if ! [ -s 'missing' ]; then
    printf '%s\n' 'missing_ok'
  else
    printf '%s\n' 'missing_bad'
  fi
}
__sh2_status=0
main "$@"
