__sh2_err_handler() { printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; }
trap '__sh2_err_handler' ERR
main() {
  __sh2_loc="/srv/repos/rust/sh2c/tests/fixtures/fs_non_empty_basic.sh2:2"
  'sh' '-c' 'rm -f empty nonempty; : > empty; echo hi > nonempty'; __sh2_status=$?; (exit $__sh2_status)
  if [ -s 'nonempty' ]; then
    printf '%s\n' 'nonempty_ok'
  else
    printf '%s\n' 'nonempty_bad'
  fi
  if ! [ -s 'empty' ]; then
    printf '%s\n' 'empty_ok'
  else
    printf '%s\n' 'empty_bad'
  fi
  if ! [ -s 'missing' ]; then
    printf '%s\n' 'missing_ok'
  else
    printf '%s\n' 'missing_bad'
  fi
}
__sh2_status=0
main "$@"
