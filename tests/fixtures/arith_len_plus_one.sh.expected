#!/usr/bin/env bash
__sh2_err_handler() { local s=$?; if [[ "${BASH_COMMAND}" == *"(exit "* ]]; then return $s; fi; printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; return $s; }
set -o errtrace
trap '__sh2_err_handler' ERR
main() {
  local __sh2_loc=""
  __sh2_loc="tests/fixtures/arith_len_plus_one.sh2:2:3"
  n="$(( ( $( printf "%s" 'hi' | awk '{ print length($0) }' ) + 1 ) ))"
  printf '%s\n' "$n"
}
__sh2_status=0
main "$@"
