#!/usr/bin/env bash
__sh2_err_handler() { local s=$?; if [[ "${BASH_COMMAND}" == *"(exit "* ]]; then return $s; fi; printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; return $s; }
set -o errtrace
trap '__sh2_err_handler' ERR
main() {
  local __sh2_loc=""
  __sh2_loc="tests/fixtures/break_basic.sh2:2:5"
  xs=('a' 'stop' 'b')
  for x in "${xs[@]}"; do
    if [ "$x" = 'stop' ]; then
      break
    fi
    printf '%s\n' "$x"
  done
}
__sh2_status=0
main "$@"
