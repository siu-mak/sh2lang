__sh2_err_handler() { printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; }
trap '__sh2_err_handler' ERR
main() {
  __sh2_loc="tests/fixtures/break_basic.sh2:2"
  xs=('a' 'stop' 'b')
  for x in "${xs[@]}"; do
    if [ "$x" = 'stop' ]; then
      break
    fi
    printf '%s\n' "$x"
  done
}
__sh2_status=0
main "$@"
