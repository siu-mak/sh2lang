#!/usr/bin/env bash
__sh2_check() { local s="$1"; local loc="$2"; local mode="$3"; if (( s != 0 )); then if [[ -n "$loc" ]]; then printf 'Error in %s\n' "$loc" >&2; fi; if [[ "$mode" == "return" ]]; then return "$s"; else exit "$s"; fi; fi; }
__sh2_uid="$(id -u 2>/dev/null || printf '%s' 0)"
main() {
  if [ "$__sh2_uid" = "${UID}" ]; then
    printf '%s\n' 'uid_ok'
  else
    printf '%s\n' 'uid_bad'
  fi
  if [ "$PPID" = "${PPID}" ]; then
    printf '%s\n' 'ppid_ok'
  else
    printf '%s\n' 'ppid_bad'
  fi
  if [ "$PWD" = "${PWD}" ]; then
    printf '%s\n' 'pwd_ok'
  else
    printf '%s\n' 'pwd_bad'
  fi
}
__sh2_status=0
main "$@"
