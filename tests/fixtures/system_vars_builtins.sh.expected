main() {
  if [ "$UID" = "${UID}" ]; then
    printf '%s\n' 'uid_ok'
  else
    printf '%s\n' 'uid_bad'
  fi
  if [ "$PPID" = "${PPID}" ]; then
    printf '%s\n' 'ppid_ok'
  else
    printf '%s\n' 'ppid_bad'
  fi
  if [ "$PWD" = "${PWD}" ]; then
    printf '%s\n' 'pwd_ok'
  else
    printf '%s\n' 'pwd_bad'
  fi
}

__sh2_status=0
main "$@"
