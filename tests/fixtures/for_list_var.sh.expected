#!/usr/bin/env bash
__sh2_err_handler() { local s=$?; if [[ "${BASH_COMMAND}" == *"(exit "* ]]; then return $s; fi; printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; return $s; }
set -o errtrace
trap '__sh2_err_handler' ERR
main() {
  local __sh2_loc=""
  __sh2_loc="tests/fixtures/for_list_var.sh2:2:3"
  xs=('a' 'b' 'c')
  for x in "${xs[@]}"; do
    printf '%s\n' "$x"
  done
}
__sh2_status=0
main "$@"
