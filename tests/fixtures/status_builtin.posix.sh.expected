#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; exit "$s"; fi; }
main() {
  __sh2_loc="tests/fixtures/status_builtin.sh2:2:3"
  'true'; __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  if [ "$__sh2_status" = "0" ]; then
    printf '%s\n' 'true_ok'
  else
    printf '%s\n' 'true_bad'
  fi
  __sh2_loc="tests/fixtures/status_builtin.sh2:9:3"
  'false'; __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  if [ "$__sh2_status" != "0" ]; then
    printf '%s\n' 'false_ok'
  else
    printf '%s\n' 'false_bad'
  fi
}
__sh2_status=0
main "$@"
