check() {
  path="${1}"
  if [ -s "$path" ]; then return 0; else return 1; fi
}

main() {
  __sh2_loc="tests/fixtures/return_fs_predicate.sh2:6"
  'sh' '-c' 'rm -f empty nonempty; printf '\'''\'' > empty; echo hi > nonempty'; __sh2_status=$?; if [ $__sh2_status -ne 0 ]; then printf 'Error in %s\n' "$__sh2_loc" >&2; exit $__sh2_status; fi
  check 'nonempty'; __sh2_status=$?; (exit $__sh2_status)
  if [ "$__sh2_status" = "0" ]; then
    printf '%s\n' 'nonempty: ok'
  fi
  check 'empty'; __sh2_status=$?; (exit $__sh2_status)
  if [ "$__sh2_status" = "1" ]; then
    printf '%s\n' 'empty: ok'
  fi
}
__sh2_status=0
main "$@"
