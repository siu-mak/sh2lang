set -o errtrace
trap '__sh2_err_handler' ERR
main() {
  local __sh2_loc=""
  __sh2_loc="tests/fixtures/map_literal_index.sh2:2:1"
  local -A m=( ['a']='1' ['b']='2' )
  local -a __sh2_keys_m=( 'a' 'b' )
  printf '%s\n' "${m['a']}"
  printf '%s\n' "${m['b']}"
}
__sh2_status=0
main "$@"
