#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; exit "$s"; fi; }
main() {
  export FOO='bar'
  printf '%s\n' "${FOO}"
  __sh2_loc="tests/fixtures/set_env_basic.sh2:4:3"
  'sh' '-c' 'test "'"$FOO"'" = bar && echo child_ok'; __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
}
__sh2_status=0
main "$@"
