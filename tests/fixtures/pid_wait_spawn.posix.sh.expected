#!/bin/sh
main() {
  ( __sh2_loc="tests/fixtures/pid_wait_spawn.sh2:2:9"; 'sh' '-c' 'exit 0' ) &
  __sh2_loc="tests/fixtures/pid_wait_spawn.sh2:3:3"
  p="$!"
  __sh2_status=$?; (exit $__sh2_status)
  wait "$p"; __sh2_status=$?; (exit $__sh2_status)
  if [ "$__sh2_status" = "0" ]; then
    printf '%s\n' 'wait_ok'
  else
    printf '%s\n' 'wait_bad'
  fi
}
__sh2_status=0
main "$@"
