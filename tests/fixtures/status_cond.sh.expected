__sh2_err_handler() { printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; }
trap '__sh2_err_handler' ERR
main() {
  __sh2_loc="/srv/repos/rust/sh2c/tests/fixtures/status_cond.sh2:2"
  'bash' '-c' 'exit 7'; __sh2_status=$?; (exit $__sh2_status)
  if [ "$__sh2_status" = "7" ]; then
    printf '%s\n' 'ok'
  else
    printf '%s\n' 'bad'
  fi
}
__sh2_status=0
