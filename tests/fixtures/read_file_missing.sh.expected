__sh2_err_handler() { printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; }
trap '__sh2_err_handler' ERR
__sh2_read_file() { cat "$1" 2>/dev/null || true; }
main() {
  __sh2_loc="tests/fixtures/read_file_missing.sh2:3"
  s="$( __sh2_read_file 'does_not_exist_xyz.txt' )"
  if [ "$s" = '' ]; then
    printf '%s\n' 'empty'
  else
    printf '%s\n' 'not empty'
  fi
}
__sh2_status=0
main "$@"
