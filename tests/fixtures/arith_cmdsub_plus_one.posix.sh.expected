#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; exit "$s"; fi; }
main() {
  __sh2_loc="tests/fixtures/arith_cmdsub_plus_one.sh2:2:3"
  n="$(( ( $( 'sh' '-c' 'printf '\''%s'\'' 41' ) + 1 ) ))"
  __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  printf '%s\n' "$n"
}
__sh2_status=0
main "$@"
