#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; mode="$3"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; if [ "$mode" = "return" ]; then return "$s"; else exit "$s"; fi; fi; }
main() {
  if ! [ -n 'x' ]; then
    printf '%s\n' 'T_NOT_X'
  else
    printf '%s\n' 'F_NOT_X'
  fi
  if ! [ -n '' ]; then
    printf '%s\n' 'T_NOT_EMPTY'
  else
    printf '%s\n' 'F_NOT_EMPTY'
  fi
}
__sh2_status=0
main "$@"
