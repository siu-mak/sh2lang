__sh2_err_handler() { printf "Error in %s\n" "${__sh2_loc:-unknown}" >&2; }
trap '__sh2_err_handler' ERR
main() {
  __sh2_loc="tests/fixtures/continue_basic.sh2:2"
  xs=('a' 'skip' 'b')
  for x in "${xs[@]}"; do
    if [ "$x" = 'skip' ]; then
      continue
    fi
    printf '%s\n' "$x"
  done
}
__sh2_status=0
main "$@"
