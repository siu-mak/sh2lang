#!/bin/sh
__sh2_check() { __sh2_s="$1"; __sh2_l="$2"; __sh2_m="$3"; if [ "$__sh2_s" -ne 0 ]; then if [ "$__sh2_m" = "return" ]; then return "$__sh2_s"; fi; if [ -n "$__sh2_l" ]; then printf 'Error in %s\n' "$__sh2_l" >&2; fi; exit "$__sh2_s"; fi; }
main() {
  __sh2_loc="tests/fixtures/truthy_capture.sh2:2:5"
  empty="$( 'printf' '%s' '' )"
  __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  if [ -n "$empty" ]; then
    printf '%s\n' 'T_EMPTY'
  else
    printf '%s\n' 'F_EMPTY'
  fi
  __sh2_loc="tests/fixtures/truthy_capture.sh2:9:5"
  full="$( 'printf' '%s' 'hi' )"
  __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  if [ -n "$full" ]; then
    printf '%s\n' 'T_FULL'
  else
    printf '%s\n' 'F_FULL'
  fi
}
__sh2_status=0
main "$@"
