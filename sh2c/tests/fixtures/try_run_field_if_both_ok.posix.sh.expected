#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; mode="$3"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; if [ "$mode" = "return" ]; then return "$s"; else exit "$s"; fi; fi; }
__sh2_tmpfile() { if command -v mktemp >/dev/null 2>&1; then mktemp; else printf "%s/sh2_tmp_%s_%s" "${TMPDIR:-/tmp}" "$$" "$(awk 'BEGIN{srand();print int(rand()*1000000)}')"; fi; }
__sh2_read_file() { cat "$1"; }
main() {
  __sh2_loc="tests/fixtures/try_run_field_if_both_ok.sh2:2:3"
  r='init'
  __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  if true; then
    __sh2_loc="tests/fixtures/try_run_field_if_both_ok.sh2:4:5"
        r__tmp_out="$(__sh2_tmpfile)"
    r__tmp_err="$(__sh2_tmpfile)"
    if 'sh' '-c' 'printf '\''a'\''; exit 0' >"${r__tmp_out}" 2>"${r__tmp_err}"; then r__status=0; else r__status=$?; fi;
    r__stdout="$(__sh2_read_file "$r__tmp_out")"
    r__stderr="$(__sh2_read_file "$r__tmp_err")"
    rm -f "$r__tmp_out" "$r__tmp_err"
    __sh2_status="$r__status"
  else
    __sh2_loc="tests/fixtures/try_run_field_if_both_ok.sh2:6:5"
        r__tmp_out="$(__sh2_tmpfile)"
    r__tmp_err="$(__sh2_tmpfile)"
    if 'sh' '-c' 'printf '\''b'\''; exit 0' >"${r__tmp_out}" 2>"${r__tmp_err}"; then r__status=0; else r__status=$?; fi;
    r__stdout="$(__sh2_read_file "$r__tmp_out")"
    r__stderr="$(__sh2_read_file "$r__tmp_err")"
    rm -f "$r__tmp_out" "$r__tmp_err"
    __sh2_status="$r__status"
  fi
  printf '%s\n' "$r__stdout"
}
__sh2_status=0
main "$@"
