#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; mode="$3"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; if [ "$mode" = "return" ]; then return "$s"; else exit "$s"; fi; fi; }
main() {
  __sh2_loc="tests/fixtures/fmt_basic.sh2:2:1"
  x="1"
  __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  if [ "$x" = "1" ]; then
    printf '%s\n' 'one'
  else
    printf '%s\n' 'not one'
  fi
  while [ "$x" -lt "10" ]; do
    __sh2_loc="tests/fixtures/fmt_basic.sh2:5:12"
    x="$(( ( x + 1 ) ))"
    __sh2_status=$?
    __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  done
  case $- in *e*) __sh2_e=1;; *) __sh2_e=0;; esac; set +e
  if {
    {
      "$ls"; __sh2_status=$?
      __sh2_check "$__sh2_status" "${__sh2_loc:-}" "return"
    }
  }; then
    if [ "${__sh2_e:-0}" = 1 ]; then set -e; fi
  else
    if [ "${__sh2_e:-0}" = 1 ]; then set -e; fi
    printf '%s\n' 'failed'
  fi
}
__sh2_status=0
main "$@"
