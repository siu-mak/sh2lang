#!/bin/sh
__sh2_check() { __sh2_s="$1"; __sh2_l="$2"; __sh2_m="$3"; if [ "$__sh2_s" -ne 0 ]; then if [ "$__sh2_m" = "return" ]; then return "$__sh2_s"; fi; if [ -n "$__sh2_l" ]; then printf 'Error in %s\n' "$__sh2_l" >&2; fi; exit "$__sh2_s"; fi; }
main() {
  __sh2_loc="tests/fixtures/status_builtin.sh2:2:3"
  'true'; __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}" "exit"
  if [ "$__sh2_status" = "0" ]; then
    printf '%s\n' 'true_ok'
  else
    printf '%s\n' 'true_bad'
  fi
  __sh2_loc="tests/fixtures/status_builtin.sh2:9:3"
  'false'; __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}" "exit"
  if [ "$__sh2_status" != "0" ]; then
    printf '%s\n' 'false_ok'
  else
    printf '%s\n' 'false_bad'
  fi
}
__sh2_status=0
main "$@"
