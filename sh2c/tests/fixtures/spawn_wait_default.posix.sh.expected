#!/bin/sh
__sh2_check() { __sh2_s="$1"; __sh2_l="$2"; __sh2_m="$3"; if [ "$__sh2_s" -ne 0 ]; then if [ "$__sh2_m" = "return" ]; then return "$__sh2_s"; fi; if [ -n "$__sh2_l" ]; then printf 'Error in %s\n' "$__sh2_l" >&2; fi; exit "$__sh2_s"; fi; }
main() {
  __sh2_loc="tests/fixtures/spawn_wait_default.sh2:4:5"
  __sh2_loc="tests/fixtures/spawn_wait_default.sh2:4:25"
  'sh' '-c' 'exit 0' &
  __sh2_status=$?
  job_pid=$!
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  __sh2_loc="tests/fixtures/spawn_wait_default.sh2:7:5"
  __sh2_loc="tests/fixtures/spawn_wait_default.sh2:7:14"
  wait "$job_pid"
  __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  rc=$__sh2_status
  printf '%s\n' 'exit code: '"$rc"
}
__sh2_status=0
main "$@"
