#!/bin/sh
__sh2_check() { __sh2_s="$1"; __sh2_l="$2"; __sh2_m="$3"; if [ "$__sh2_s" -ne 0 ]; then if [ "$__sh2_m" = "return" ]; then return "$__sh2_s"; fi; if [ -n "$__sh2_l" ]; then printf 'Error in %s\n' "$__sh2_l" >&2; fi; exit "$__sh2_s"; fi; }
main() {
  __sh2_loc="tests/fixtures/fs_exists_dir_file_symlink_basic.sh2:2:3"
  'sh' '-c' 'rm -rf tmp; mkdir -p tmp/dir; : > tmp/file; ln -s file tmp/link'; __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}" "exit"
  if [ -e 'tmp/file' ] && [ -f 'tmp/file' ] && [ -d 'tmp/dir' ] && [ -L 'tmp/link' ]; then
    printf '%s\n' 'ok'
  else
    printf '%s\n' 'bad'
  fi
}
__sh2_status=0
main "$@"
