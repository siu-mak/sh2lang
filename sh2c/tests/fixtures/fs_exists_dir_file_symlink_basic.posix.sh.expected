#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; mode="$3"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; if [ "$mode" = "return" ]; then return "$s"; else exit "$s"; fi; fi; }
main() {
  __sh2_loc="tests/fixtures/fs_exists_dir_file_symlink_basic.sh2:2:3"
  'sh' '-c' 'rm -rf tmp; mkdir -p tmp/dir; : > tmp/file; ln -s file tmp/link'; __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}" "exit"
  if [ -e 'tmp/file' ] && [ -f 'tmp/file' ] && [ -d 'tmp/dir' ] && [ -L 'tmp/link' ]; then
    printf '%s\n' 'ok'
  else
    printf '%s\n' 'bad'
  fi
}
__sh2_status=0
main "$@"
