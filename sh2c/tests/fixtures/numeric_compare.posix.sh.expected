#!/bin/sh
__sh2_check() { __sh2_s="$1"; __sh2_l="$2"; __sh2_m="$3"; if [ "$__sh2_s" -ne 0 ]; then if [ "$__sh2_m" = "return" ]; then return "$__sh2_s"; fi; if [ -n "$__sh2_l" ]; then printf 'Error in %s\n' "$__sh2_l" >&2; fi; exit "$__sh2_s"; fi; }
main() {
  if [ "1" -lt "2" ]; then
    printf '%s\n' 'lt_ok'
  else
    printf '%s\n' 'lt_bad'
  fi
  if [ "2" -le "2" ]; then
    printf '%s\n' 'le_ok'
  else
    printf '%s\n' 'le_bad'
  fi
  if [ "3" -gt "2" ]; then
    printf '%s\n' 'gt_ok'
  else
    printf '%s\n' 'gt_bad'
  fi
  if [ "3" -ge "3" ]; then
    printf '%s\n' 'ge_ok'
  else
    printf '%s\n' 'ge_bad'
  fi
}
__sh2_status=0
main "$@"
