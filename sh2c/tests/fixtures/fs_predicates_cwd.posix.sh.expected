#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; mode="$3"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; if [ "$mode" = "return" ]; then return "$s"; else exit "$s"; fi; fi; }
__sh2_err_handler() {
  s=$?
  loc="${__sh2_loc:-}"
  if [ -z "$loc" ]; then return "$s"; fi
  if [ "$loc" = "${__sh2_last_err_loc:-}" ] && [ "$s" = "${__sh2_last_err_status:-}" ]; then return "$s"; fi
  __sh2_last_err_loc="$loc"
  __sh2_last_err_status="$s"
  printf "Error in %s\n" "$loc" >&2
  return "$s"
}
if [ -n "${BASH_VERSION:-}" ]; then set -o errtrace; fi
trap '__sh2_err_handler' ERR
main() {
  __sh2_loc="tests/fixtures/fs_predicates_cwd.sh2:3:3"
  'mkdir' '-p' 'sh2_test_scratch_fs_predicates_cwd'; __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}" "exit"
  (
    cd 'sh2_test_scratch_fs_predicates_cwd'
    __sh2_loc="tests/fixtures/fs_predicates_cwd.sh2:5:5"
    __sh2_cmd=''
    __sh2_cmd="${__sh2_cmd}touch f.txt"
    __sh2_status=0; 'sh' -c "$__sh2_cmd" "_" "$@" || __sh2_status=$?
    __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  )
  __sh2_loc="tests/fixtures/fs_predicates_cwd.sh2:8:3"
  f='sh2_test_scratch_fs_predicates_cwd/f.txt'
  __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  if [ -e "$f" ] && [ -f "$f" ] && ! [ -d "$f" ]; then
    printf '%s\n' 'fs_ok'
  else
    printf '%s\n' 'fs_bad'
  fi
  __sh2_loc="tests/fixtures/fs_predicates_cwd.sh2:17:3"
  d="$( 'mktemp' '-d' )"
  __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  cd "$d"
  if [ "$PWD" = "$d" ]; then
    printf '%s\n' 'cd_ok'
  else
    printf '%s\n' 'cd_bad'
  fi
  __sh2_loc="tests/fixtures/fs_predicates_cwd.sh2:26:3"
  'rm' '-rf' "$d"; __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}" "exit"
  __sh2_loc="tests/fixtures/fs_predicates_cwd.sh2:27:3"
  'rm' '-rf' 'sh2_test_scratch_fs_predicates_cwd'; __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}" "exit"
}
__sh2_status=0
main "$@"
