#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; mode="$3"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; if [ "$mode" = "return" ]; then return "$s"; else exit "$s"; fi; fi; }
main() {
  __sh2_loc="tests/fixtures/wait_list_var.sh2:2:3"
  a='123'
  __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  __sh2_loc="tests/fixtures/wait_list_var.sh2:3:3"
  b='456'
  __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  wait "$a" "$b"; __sh2_status=$?
__sh2_check "$__sh2_status" "${{__sh2_loc:-}}"
}
__sh2_status=0
main "$@"
