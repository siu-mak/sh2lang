#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; mode="$3"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; if [ "$mode" = "return" ]; then return "$s"; else exit "$s"; fi; fi; }
main() {
  __sh2_loc="tests/fixtures/while_basic.sh2:2:5"
  x='go'
  __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  while [ "$x" = 'go' ]; do
    printf '%s\n' 'loop'
    __sh2_loc="tests/fixtures/while_basic.sh2:5:9"
    x='stop'
    __sh2_status=$?
    __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  done
}
__sh2_status=0
main "$@"
