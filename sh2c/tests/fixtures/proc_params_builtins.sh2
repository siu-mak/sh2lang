func main() {
    // argv0() returns the script name, arg(0) is invalid (returns empty)
    if len(argv0()) > 0 {
        print("argv0_ok")
    } else {
        print("argv0_bad")
    }

    if argc() == count(args) {
        print("argc_ok")
    } else {
        print("argc_bad")
    }

    if self_pid() == env("BASHPID") {
        print("self_pid_ok")
    } else {
        print("self_pid_bad")
    }
}
