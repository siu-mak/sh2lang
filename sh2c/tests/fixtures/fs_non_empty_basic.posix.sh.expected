#!/bin/sh
__sh2_check() { __sh2_s="$1"; __sh2_l="$2"; __sh2_m="$3"; if [ "$__sh2_s" -ne 0 ]; then if [ "$__sh2_m" = "return" ]; then return "$__sh2_s"; fi; if [ -n "$__sh2_l" ]; then printf 'Error in %s\n' "$__sh2_l" >&2; fi; exit "$__sh2_s"; fi; }
main() {
  __sh2_loc="tests/fixtures/fs_non_empty_basic.sh2:2:5"
  'sh' '-c' 'rm -f empty nonempty; : > empty; echo hi > nonempty'; __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}" "exit"
  if [ -s 'nonempty' ]; then
    printf '%s\n' 'nonempty_ok'
  else
    printf '%s\n' 'nonempty_bad'
  fi
  if ! [ -s 'empty' ]; then
    printf '%s\n' 'empty_ok'
  else
    printf '%s\n' 'empty_bad'
  fi
  if ! [ -s 'missing' ]; then
    printf '%s\n' 'missing_ok'
  else
    printf '%s\n' 'missing_bad'
  fi
}
__sh2_status=0
main "$@"
