#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; mode="$3"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; if [ "$mode" = "return" ]; then return "$s"; else exit "$s"; fi; fi; }
main() {
  ( __sh2_loc="tests/fixtures/wait_all.sh2:2:9"; 'sleep' '1' ) &
  ( __sh2_loc="tests/fixtures/wait_all.sh2:3:9"; 'sleep' '1' ) &
  wait; __sh2_status=$?
  printf '%s\n' 'done'
}
__sh2_status=0
main "$@"
