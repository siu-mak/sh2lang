#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; mode="$3"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; if [ "$mode" = "return" ]; then return "$s"; else exit "$s"; fi; fi; }
__sh2_err_handler() {
  s=$?
  loc="${__sh2_loc:-}"
  if [ -z "$loc" ]; then return "$s"; fi
  if [ "$loc" = "${__sh2_last_err_loc:-}" ] && [ "$s" = "${__sh2_last_err_status:-}" ]; then return "$s"; fi
  __sh2_last_err_loc="$loc"
  __sh2_last_err_status="$s"
  printf "Error in %s\n" "$loc" >&2
  return "$s"
}
if [ -n "${BASH_VERSION:-}" ]; then set -o errtrace; fi
trap '__sh2_err_handler' ERR
main() {
  export X='hello'
  __sh2_loc="tests/fixtures/env_export_unset_source.sh2:4:3"
  __sh2_cmd=''
  __sh2_cmd="${__sh2_cmd}echo \$X"
  __sh2_status=0; 'sh' -c "$__sh2_cmd" "_" "$@" || __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  __sh2_loc="tests/fixtures/env_export_unset_source.sh2:7:3"
  y='yo'
  __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  export y
  __sh2_loc="tests/fixtures/env_export_unset_source.sh2:9:3"
  __sh2_cmd=''
  __sh2_cmd="${__sh2_cmd}echo \$y"
  __sh2_status=0; 'sh' -c "$__sh2_cmd" "_" "$@" || __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  unset X
  __sh2_loc="tests/fixtures/env_export_unset_source.sh2:13:3"
  __sh2_cmd=''
  __sh2_cmd="${__sh2_cmd}test -z \"\$X\" && echo unset_ok || echo unset_bad"
  __sh2_status=0; 'sh' -c "$__sh2_cmd" "_" "$@" || __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  __sh2_loc="tests/fixtures/env_export_unset_source.sh2:16:3"
  f="$( 'mktemp' )"
  __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  __sh2_loc="tests/fixtures/env_export_unset_source.sh2:17:3"
  __sh2_cmd=''
  __sh2_cmd="${__sh2_cmd}echo 'echo sourced_ok' > "
  __sh2_cmd="${__sh2_cmd}$f"
  __sh2_status=0; 'sh' -c "$__sh2_cmd" "_" "$@" || __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  . "$f"
  __sh2_loc="tests/fixtures/env_export_unset_source.sh2:21:3"
  'rm' '-f' "$f"; __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}" "exit"
}
__sh2_status=0
main "$@"
