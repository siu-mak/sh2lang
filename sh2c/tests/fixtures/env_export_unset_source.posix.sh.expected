#!/bin/sh
__sh2_check() { __sh2_s="$1"; __sh2_l="$2"; __sh2_m="$3"; if [ "$__sh2_s" -ne 0 ]; then if [ "$__sh2_m" = "return" ]; then return "$__sh2_s"; fi; if [ -n "$__sh2_l" ]; then printf 'Error in %s\n' "$__sh2_l" >&2; fi; exit "$__sh2_s"; fi; }
__sh2_sh_probe() { cmd="$1"; if sh -c "$cmd"; then __sh2_status=0; else __sh2_status=$?; fi; return 0; }
main() {
  export X='hello'
  __sh2_loc="tests/fixtures/env_export_unset_source.sh2:4:3"
  'sh' '-c' 'echo $X'; __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}" "exit"
  __sh2_loc="tests/fixtures/env_export_unset_source.sh2:7:3"
  y='yo'
  __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  export y
  __sh2_loc="tests/fixtures/env_export_unset_source.sh2:9:3"
  'sh' '-c' 'echo $y'; __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}" "exit"
  unset X
  __sh2_loc="tests/fixtures/env_export_unset_source.sh2:13:3"
  'sh' '-c' 'test -z "$X" && echo unset_ok || echo unset_bad'; __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}" "exit"
  __sh2_loc="tests/fixtures/env_export_unset_source.sh2:16:3"
  f="$( 'mktemp' )"
  __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  __sh2_loc="tests/fixtures/env_export_unset_source.sh2:17:3"
  __sh2_cmd='echo '\''echo sourced_ok'\'' > '"$f"
  __sh2_sh_probe "$__sh2_cmd"
  . "$f"
  __sh2_loc="tests/fixtures/env_export_unset_source.sh2:21:3"
  'rm' '-f' "$f"; __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}" "exit"
}
__sh2_status=0
main "$@"
