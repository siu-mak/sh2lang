#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; mode="$3"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; if [ "$mode" = "return" ]; then return "$s"; else exit "$s"; fi; fi; }
__sh2_coalesce() { if [ -n "$1" ]; then printf '%s' "$1"; else printf '%s' "$2"; fi; }
main() {
  printf '%s\n' "$( __sh2_coalesce '' 'fallback' )"
  printf '%s\n' "$( __sh2_coalesce 'x' 'fallback' )"
}
__sh2_status=0
main "$@"
