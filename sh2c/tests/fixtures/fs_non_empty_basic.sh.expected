#!/usr/bin/env bash
__sh2_check() { local s="$1"; local loc="$2"; local mode="$3"; if (( s != 0 )); then if [[ "$mode" == "return" ]]; then return "$s"; else if [[ -n "$loc" ]]; then printf 'Error in %s\n' "$loc" >&2; fi; exit "$s"; fi; fi; }
main() {
  __sh2_loc="tests/fixtures/fs_non_empty_basic.sh2:2:5"
  __sh2_cmd=''
  __sh2_cmd+='rm -f empty nonempty; : > empty; echo hi > nonempty'
  __sh2_status=0; 'sh' -c "$__sh2_cmd" "_" "$@" || __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  if [ -s 'nonempty' ]; then
    printf '%s\n' 'nonempty_ok'
  else
    printf '%s\n' 'nonempty_bad'
  fi
  if ! [ -s 'empty' ]; then
    printf '%s\n' 'empty_ok'
  else
    printf '%s\n' 'empty_bad'
  fi
  if ! [ -s 'missing' ]; then
    printf '%s\n' 'missing_ok'
  else
    printf '%s\n' 'missing_bad'
  fi
}
__sh2_status=0
main "$@"
