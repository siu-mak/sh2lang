#!/bin/sh
__sh2_check() { s="$1"; loc="$2"; mode="$3"; if [ "$s" -ne 0 ]; then if [ -n "$loc" ]; then printf 'Error in %s\n' "$loc" >&2; fi; if [ "$mode" = "return" ]; then return "$s"; else exit "$s"; fi; fi; }
main() {
  __sh2_loc="tests/fixtures/subshell_group_scope.sh2:2:3"
  x='0'
  __sh2_status=$?
  __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  (
    __sh2_loc="tests/fixtures/subshell_group_scope.sh2:5:5"
    x='1'
    __sh2_status=$?
    __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  )
  printf '%s\n' "$x"
  {
    __sh2_loc="tests/fixtures/subshell_group_scope.sh2:10:5"
    x='2'
    __sh2_status=$?
    __sh2_check "$__sh2_status" "${__sh2_loc:-}"
  }
  printf '%s\n' "$x"
}
__sh2_status=0
main "$@"
