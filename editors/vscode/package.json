{
    "name": "sh2",
    "displayName": "sh2",
    "description": "Syntax highlighting for sh2 shell scripting language",
    "version": "0.1.2",
    "icon": "images/sh2logo_256.png",
    "publisher": "siu-mak",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/siu-mak/sh2lang.git"
    },
    "homepage": "https://github.com/siu-mak/sh2lang",
    "bugs": {
        "url": "https://github.com/siu-mak/sh2lang/issues"
    },
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "sh2",
                "aliases": [
                    "sh2",
                    "SH2"
                ],
                "extensions": [
                    ".sh2"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "sh2",
                "scopeName": "source.sh2",
                "path": "./syntaxes/sh2.tmLanguage.json"
            }
        ]
    }
}